#include <iostream>
#include <string>
using namespace std;
 
int main(void)
{
	int num = 0;
    
	string s;
	cin >> s;
 
	for (int i = 0; i < s.length(); i++)
    {
        
		num *= 16;
 
		if (s[i] >= 'A' && s[i] <= 'F')
			num = num + s[i] - 55;
		else
			num = num + s[i] - 48;
        
	}
	cout << num << endl;
 
	return 0;
}
